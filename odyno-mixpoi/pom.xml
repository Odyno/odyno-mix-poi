<?xml version="1.0" encoding="UTF-8"?>
<project>
    <modelVersion>4.0.0</modelVersion>
    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    </properties>

    <groupId>net.staniscia.sw</groupId>
    <artifactId>odyno-mixpoi</artifactId>
    <version>0.0.1-SNAPSHOT</version>
    <name>odyno-mixpoi</name>
    <description>This plugin transform your Worpress Site in your DataSource-Server for Mixare Apps. Odyno MixPoi use all wordpress container feature and it use the OpenLayer presentation feature.</description>
    <url>http://www.staniscia.net</url>

    
    <build>
        <plugins>
            <plugin>
                <artifactId>maven-assembly-plugin</artifactId>
                <version>2.4</version>
                <configuration>
                    <descriptor>src/res/assembly.xml</descriptor>
                </configuration>
            </plugin>
        </plugins>
    </build>

</project>
